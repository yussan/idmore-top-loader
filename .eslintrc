{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "strict": [2, "never"],
    "semi": [1, "never"],
    "no-underscore-dangle": 0
  },
  "plugins": [
  ]
}
